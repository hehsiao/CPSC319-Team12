<div class="modal fade" id="new_partner_form_<%=form_number%>" tabindex="-1" role="dialog" aria-labelledby="new_partner_form_2_label" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                <h4 class="modal-title" id="myModalLabel">Modal title</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
				  <%= label_tag :partner_name, "Partner Name" %><br>
				  <%= text_field :"partner[#{form_number}]", :partner_name %>
				</div>
				<div class="row">
				  <div class="col-md-6">
				    <h4>Primary Contact</h4>
				    <div class="form-group">
				      <%= label_tag :contact_name, "Contact Name" %><br>
				      <%= text_field :"partner[#{form_number}]",:contact_name, :class => 'form-control' %>
				    </div>
				    <div class="form-group">
				      <%= label_tag :email, "Email" %><br>
				      <%= email_field :"partner[#{form_number}]", :email, :class => 'form-control'  %>
				    </div>
				    <div class="form-group">
				      <%= label_tag :phone_num, "Phone Number" %><br>
				      <%= phone_field :"partner[#{form_number}]", :phone_num, :class => 'form-control' %>
				    </div>
				  </div>
				  <div class="col-md-6">
				    <h4>Secondary Contact</h4>
				    <div class="form-group">
				      <%= label_tag :contact_name, "Contact Name" %><br>
				      <%= text_field :"partner[#{form_number}]", :secondary_contact_name, :class => 'form-control' %>
				    </div>
				    <div class="form-group">
				      <%= label_tag :email, "Email" %><br>
				      <%= text_field :"partner[#{form_number}]",:secondary_email, :class => 'form-control' %>
				    </div>
				    <div class="form-group">
				      <%= label_tag :phone_num, "Phone Number" %><br>
				      <%= text_field :"partner[#{form_number}]", :secondary_phone_num, :class => 'form-control' %>
				    </div>
				  </div>
				</div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>